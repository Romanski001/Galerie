{% extends 'base.html.twig' %}

{% block body %}
    <h1>Autrestechnique</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ autresTechnique.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ autresTechnique.nom }}</td>
            </tr>
            <tr>
                <th>Détails</th>
                <td>{{ autresTechnique.détails }}</td>
            </tr>
            <tr>
                <th>Dimensions</th>
                <td>{{ autresTechnique.dimensions }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if autresTechnique.date %}{{ autresTechnique.date|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('autrestechniques_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('autrestechniques_edit', { 'id': autresTechnique.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
